            "3", "4", "5",
            "6", "7", "8",
            "9",
            "..", "0x8000.."
            },
        Values {
            "Not Specified", "Other", "Implementation decides",
            "Full Copy", "Incremental-Copy", "Differential-Copy",
            "Copy-On-Write", "Copy-On-Access", "Delta-Update",
            "Snap-And-Clone",
            "Microsoft Reserved", "Vendor Specific"
            }]
